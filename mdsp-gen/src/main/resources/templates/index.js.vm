import request from '@/utils/request'

export function page${tableInfo.className}Api(query) {
  return request({
    url: '/${moduleName}/${secondModuleName}/page',
    method: 'get',
    params: query
  })
}

export function add${tableInfo.className}Api(obj) {
  return request({
    url: '/${moduleName}/${secondModuleName}/add',
    method: 'post',
    data: obj
  })
}

export function get${tableInfo.className}Api(id) {
  return request({
    url: '/${moduleName}/${secondModuleName}/get?id=' + id,
    method: 'get'
  })
}

export function del${tableInfo.className}Api(id) {
  return request({
    url: '/${moduleName}/${secondModuleName}/delete?id=' + id,
    method: 'post'
  })
}

export function update${tableInfo.className}Api(obj) {
  return request({
    url: '/${moduleName}/${secondModuleName}/update',
    method: 'post',
    data: obj
  })
}

